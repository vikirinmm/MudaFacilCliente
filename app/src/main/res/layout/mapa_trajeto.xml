<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#C1C1C1">

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView2"
        android:layout_width="376dp"
        android:layout_height="425dp"
        android:background="#C1C1C1"
        app:layout_constraintBottom_toTopOf="@+id/menu_azul"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cabecalho" />

    <View
        android:id="@+id/cabecalho"
        android:layout_width="400dp"
        android:layout_height="73dp"
        android:layout_marginTop="35dp"
        android:background="@drawable/cabecalho"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/menu_azul"
        android:layout_width="match_parent"
        android:layout_height="304dp"
        android:background="@drawable/cabecalhobaixo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/voltar_mapa_trajeto"
        android:layout_width="55dp"
        android:layout_height="53dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/cabecalho"
        app:layout_constraintStart_toStartOf="@+id/cabecalho"
        app:layout_constraintTop_toTopOf="@+id/cabecalho"
        app:srcCompat="@drawable/voltarbranco" />

    <View
        android:id="@+id/pesquisa"
        android:layout_width="254dp"
        android:layout_height="51dp"
        android:background="@drawable/pesquisa"
        app:layout_constraintBottom_toBottomOf="@+id/cabecalho"
        app:layout_constraintEnd_toEndOf="@+id/cabecalho"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/cabecalho"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/textopesquisa"
        android:layout_width="240dp"
        android:layout_height="48dp"
        android:backgroundTint="#3473D2"
        android:ems="10"
        android:inputType="text"
        android:textColor="@color/black"
        android:textColorHint="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/pesquisa"
        app:layout_constraintEnd_toEndOf="@+id/pesquisa"
        app:layout_constraintStart_toStartOf="@+id/pesquisa"
        app:layout_constraintTop_toTopOf="@+id/pesquisa"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="62dp"
        android:layout_height="62dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/cabecalho"
        app:layout_constraintEnd_toEndOf="@+id/cabecalho"
        app:layout_constraintTop_toTopOf="@+id/cabecalho"
        app:layout_constraintVertical_bias="0.909"
        app:srcCompat="@drawable/lupa" />

    <View
        android:id="@+id/menu"
        android:layout_width="375dp"
        android:layout_height="238dp"
        android:background="@drawable/recentes"
        app:layout_constraintBottom_toBottomOf="@+id/menu_azul"
        app:layout_constraintEnd_toEndOf="@+id/menu_azul"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="@+id/menu_azul"
        app:layout_constraintTop_toTopOf="@+id/menu_azul"
        app:layout_constraintVertical_bias="0.257" />

    <View
        android:id="@+id/bloco_menu1"
        android:layout_width="350dp"
        android:layout_height="85dp"
        android:layout_marginTop="12dp
"
        android:background="@drawable/recentes"
        app:layout_constraintEnd_toEndOf="@+id/menu"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="@+id/menu"
        app:layout_constraintTop_toTopOf="@+id/menu" />

    <View
        android:id="@+id/bloco_menu2"
        android:layout_width="350dp"
        android:layout_height="115dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/recentes"
        app:layout_constraintEnd_toEndOf="@+id/menu"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="@+id/menu"
        app:layout_constraintTop_toBottomOf="@+id/bloco_menu1" />

    <TextView
        android:id="@+id/cancelarpedido"
        android:layout_width="340dp"
        android:layout_height="56dp"
        android:gravity="center"
        android:text="cancelar"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/bloco_menu1"
        app:layout_constraintEnd_toEndOf="@+id/bloco_menu1"
        app:layout_constraintStart_toStartOf="@+id/bloco_menu1"
        app:layout_constraintTop_toTopOf="@+id/bloco_menu1" />

    <TextView
        android:id="@+id/endereco"
        android:layout_width="340dp"
        android:layout_height="80dp"
        android:gravity="center"
        android:text="EndereÃ§o"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/bloco_menu2"
        app:layout_constraintEnd_toEndOf="@+id/bloco_menu2"
        app:layout_constraintStart_toStartOf="@+id/bloco_menu2"
        app:layout_constraintTop_toTopOf="@+id/bloco_menu2"
        app:layout_constraintVertical_bias="0.542" />

    <View
        android:id="@+id/view12"
        android:layout_width="376dp"
        android:layout_height="425dp"
        android:background="@drawable/mapa_borda"
        app:layout_constraintEnd_toEndOf="@+id/mapView2"
        app:layout_constraintTop_toTopOf="@+id/mapView2" />

</androidx.constraintlayout.widget.ConstraintLayout>